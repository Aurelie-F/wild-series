{% extends 'base.html.twig' %}

{% block title %}{{ parent() }} - New category{% endblock %}

{% block body %}
    <div class="container-fluid ml-4">
        <h1 class="text-center my-4">Add a new category</h1>
        {{ form_start(form) }}
        {{ form_widget(form) }}
        <button type="submit" class="btn btn-primary">Add category</button>
        {{ form_end(form) }}
    </div>
{% endblock %}